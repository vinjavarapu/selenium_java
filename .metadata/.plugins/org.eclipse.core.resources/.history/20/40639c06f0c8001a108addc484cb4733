package Varibles;



public class Varibles_1 {
	
	static int constant = 10;
	
	public void method1() {
		
		int constant = 20;		
		System.out.println("The varible value is " + constant);
		
	}
	public static void manin(String args[])
	{
		Varibles_1 obj = new Varibles_1();
		obj.method1();
		System.out.println("The class varibles is " + constant);
	}

}